{
  "name": "xstate-viz-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://xstate.js.org/viz",
  "dependencies": {
    "@statecharts/xstate-viz": "^0.3.0",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.1",
    "@xstate/react": "^0.6.1",
    "query-string": "^6.8.0",
    "react-scripts": "^3.0.1",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "shx cp ./src/indexDefault.tsx ./src/index.tsx && react-scripts start",
    "previewOnly": "shx cp ./src/indexPreviewOnly.tsx ./src/index.tsx && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "react-scripts build",
    "deploy": "./node_modules/.bin/gh-pages -d build --repo https://github.com/davidkpiano/xstate.git --dest viz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.12",
    "gh-pages": "^2.0.1",
    "shx": "^0.3.2",
    "typescript": "^3.4.5"
  }
}
